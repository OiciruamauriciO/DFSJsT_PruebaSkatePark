    <h1>{{profileTitle}}</h1>

    <div class="py-4">
      <h2>{{profileSubTitle}}</h2>
      <hr class="w-50" />

      <form if="profileForm" name="profileForm" method="" action="" enctype="multipart/form-data">
        <div class="form-group row w-50 m-auto">
          <div class="form-group col-12 col-sm-6">
            <label>Email</label>
            <input id="email" name="email" class="form-control m-auto" value="{{{profileToViewData.email}}}"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Nombre</label>
            <input id="nombre" name="nombre" class="form-control m-auto" value="{{{profileToViewData.nombre}}}"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Password</label>
            <input id="password" name="password" type="password" class="form-control m-auto" value="{{{profileToViewData.password}}}"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Repita la password</label>
            <input id="repassword" name="repassword" type="password"  class="form-control m-auto" value="{{{profileToViewData.password}}}"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>AÃ±os de experiencia</label>
            <input id="anos_experiencia" name="anos_experiencia" class="form-control m-auto" value="{{profileToViewData.anos_experiencia}}"/>
          </div>
          <div class="form-group col-12 col-sm-6">
            <label>Especialidad</label>
            <input id="especialidad" name="especialidad" class="form-control m-auto" value="{{{profileToViewData.especialidad}}}"/>
          </div>
        </div>
        <button type="button" class="btn btn-primary btn-sm" onclick="actualizarPerfil(this)">Actualizar</button>
        <button type="button" class="btn btn-danger btn-sm" onclick="eliminarPerfil(this)">Eliminar cuenta</button>
      </form>
    </div>